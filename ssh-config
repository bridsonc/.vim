Host dev
  Hostname n03.devhost.dev.liquidweb.com
  User cbridson
  IdentityFile ~/.ssh/dev_env

Host *.int
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes
    HostName %h.liquidweb.com
Host *.int.*
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes
Host *.dev
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes
    HostName %h.liquidweb.com
Host *.dev.*
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes
Host *.staging
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes
    HostName %h.liquidweb.com
Host *.staging.*
    User root
    IdentityFile ~/.ssh/dev_env
    ForwardAgent yes

Host *
  ForwardAgent yes
  ForwardX11 yes
  ForwardX11Trusted yes
  ServerAliveInterval 1800
